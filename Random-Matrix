import java.util.Scanner;

	
	public static void printMatrix(int n) {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j ++) {
				
				System.out.print((int)(Math.random() * 2));
			}
		
			System.out.println();
			
			
		}
	}
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int user1;
		System.out.println("Enter n: ");
		user1 = input.nextInt();
		
		printMatrix(user1);
	}
